[{"create-time":1611998105556,"title":"aligning incentives",":create/user":{":user/uid":"ZP6qOGnCTgS3uoT30U1tRsUxUVv2"},"children":[{"string":"Title","create-time":1612429996299,":create/user":{":user/uid":"ZP6qOGnCTgS3uoT30U1tRsUxUVv2"},"children":[{"string":"Aligning Incentives","create-time":1612430004903,"uid":"4qGRQm-YT","edit-time":1612430010967,":create/user":{":user/uid":"ZP6qOGnCTgS3uoT30U1tRsUxUVv2"},":edit/user":{":user/uid":"ZP6qOGnCTgS3uoT30U1tRsUxUVv2"}}],"uid":"tLB-GgbD4","edit-time":1612430004863,":edit/user":{":user/uid":"ZP6qOGnCTgS3uoT30U1tRsUxUVv2"}},{"string":"Description","create-time":1612430011577,":create/user":{":user/uid":"ZP6qOGnCTgS3uoT30U1tRsUxUVv2"},"children":[{"string":"","create-time":1612433918500,"uid":"3X6bAgSL1","edit-time":1612433918500,":create/user":{":user/uid":"ZP6qOGnCTgS3uoT30U1tRsUxUVv2"},":edit/user":{":user/uid":"ZP6qOGnCTgS3uoT30U1tRsUxUVv2"}}],"uid":"_leeklC3l","edit-time":1612430015449,":edit/user":{":user/uid":"ZP6qOGnCTgS3uoT30U1tRsUxUVv2"}},{"string":"Body","create-time":1612433903024,":create/user":{":user/uid":"ZP6qOGnCTgS3uoT30U1tRsUxUVv2"},"children":[{"string":"In order to remain a [[creator first]] platform well into the future, we need to align our business incentives with that of our creators. To do this we simply tie our business model to theirs i.e. we only make money when our creators make money. Practically speaking we will be releasing high-quality, built-in monetization options for our creators beyond simple tipping. This can include tools for selling information products, private communities, e-shops etc. Our business model is to charge a percentage fee on earnings from these features.","create-time":1611998142397,":block/refs":[{":block/uid":"ap6OarYgM"}],"refs":[{"uid":"ap6OarYgM"}],":create/user":{":user/uid":"ZP6qOGnCTgS3uoT30U1tRsUxUVv2"},"uid":"qbeiIKobN","edit-time":1612516045830,":edit/user":{":user/uid":"ZP6qOGnCTgS3uoT30U1tRsUxUVv2"}}],"uid":"cPoyEhaqe","edit-time":1612433908967,":edit/user":{":user/uid":"ZP6qOGnCTgS3uoT30U1tRsUxUVv2"}},{"string":"","create-time":1612433897754,"uid":"MwJRHFT3V","edit-time":1612433897754,":create/user":{":user/uid":"ZP6qOGnCTgS3uoT30U1tRsUxUVv2"},":edit/user":{":user/uid":"ZP6qOGnCTgS3uoT30U1tRsUxUVv2"}}],"uid":"ZC54zH0BL","edit-time":1611998105556,":edit/user":{":user/uid":"ZP6qOGnCTgS3uoT30U1tRsUxUVv2"}}]